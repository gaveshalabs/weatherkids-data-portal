  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "start:dev": "ng serve --configuration dev",
    "start:prod": "ng serve --configuration prod",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:prod": "ng build --configuration prod",
    "deploy:dev": "ng build --configuration=dev && firebase deploy --only hosting --project dev",
    "deploy:prod": "ng build --configuration=prod && firebase deploy --only hosting --project prod",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "eslint -c .eslintrc.json --ext .ts .",
    "lint:fix": "eslint -c .eslintrc.json --ext .ts --fix .",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
